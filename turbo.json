{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**",
                "bin/**"
            ]
        },
        "zoekt:webserver": {
            "persistent": true,
            "cache": false
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    },
    "envMode": "loose"
}